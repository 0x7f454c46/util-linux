msgstr "%s -Un perifèric\t activa la partició n, desactiva la resta\n"

#: fdisk/sfdisk.c:2421
msgid "no command?\n"
msgstr "cap comandament?\n"

#: fdisk/sfdisk.c:2539
#, c-format
msgid "total: %d blocks\n"
msgstr "total: %d blocs\n"

#: fdisk/sfdisk.c:2576
